<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="clinica-unit"
		transaction-type="RESOURCE_LOCAL">

		<class>it.uniroma3.siw_progetto.model.Administrator</class>
		<class>it.uniroma3.siw_progetto.model.Prerequisito</class>
		<class>it.uniroma3.siw_progetto.model.Risultato</class>
		<class>it.uniroma3.siw_progetto.model.TipoEsame</class>
		<class>it.uniroma3.siw_progetto.model.Paziente</class>
		<class>it.uniroma3.siw_progetto.model.EsamePrenotato</class>
		<class>it.uniroma3.siw_progetto.model.EsameEffettuato</class>
		<class>it.uniroma3.siw_progetto.model.Medico</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
			<property name="javax.persistence.jdbc.user" value="postgres" />
			<property name="javax.persistence.jdbc.password" value="asroma" />
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost/clinica" />


			<property name="hibernate.show_sql" value="true" />
			<property name="hibernate.format_sql" value="true" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />

		</properties>


	</persistence-unit>
</persistence>